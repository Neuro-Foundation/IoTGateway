			<dependency id="Waher.Networking.XMPP" version="3.2.1"/>
			<dependency id="Waher.Persistence" version="1.12.0"/>
			<dependency id="Waher.Runtime.Inventory" version="1.1.7"/>
			<dependency id="Waher.Runtime.Language" version="1.1.4"/>
			<dependency id="Waher.Script" version="2.7.0"/>
			<dependency id="Waher.Things" version="2.0.3"/>
			<dependency id="Waher.Things.Metering" version="1.2.8"/>
			<dependency id="Windows-Remote-Arduino" version="1.4.0"/>
		</dependencies>
