	<elements>
		<Paragraph>
			<InlineText>Ordered lists use numbers followed by periods:</InlineText>
		</Paragraph>
		<NumberedList>
			<NumberedItem number="1" explicit="true">
				<InlineText>Bird</InlineText>
			</NumberedItem>
